<table>
  <thead>
  <tr class="admin-head">
    <th class="text-center">Title</th>
    <th class="text-center">URL</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <% videos.each do |v| %>
    <tr class="admin-row">
      <td class="text-center"><%= v.title %></td>
      <td><%= rails_blob_path(v.video, disposition: "attachment") %></td>
    </tr>
  <% end %>
</table>
